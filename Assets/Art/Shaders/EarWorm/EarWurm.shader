		[HideInInspector] _AlphaCutoff("Alpha Cutoff ", Range(0, 1)) = 0.5
		[HideInInspector] _EmissionColor("Emission Color", Color) = (1,1,1,1)
		_Albedo("Albedo", 2D) = "white" {}
		_AlbedoScrollSpeed("AlbedoScrollSpeed", Vector) = (0,1,0,0)
		_NormalMap("Normal Map", 2D) = "white" {}
		_normalint("normal int", Range( 0 , 1)) = 1
		_Specular("Specular", 2D) = "white" {}
		_vertexWave("vertex Wave", 2D) = "white" {}
		_VertexScrollSpeed("Vertex Scroll Speed", Float) = -0.1
		_VertexMax("Vertex Max", Float) = -1
		_VertexMin("Vertex Min", Float) = 1

